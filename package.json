{
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"nodemon server.js\" \"cd Repository && npm run dev\"",
    "dev:main": "nodemon server.js",
    "dev:repo": "cd Repository && npm run dev",
    "dev:all": "concurrently \"npm run dev:main\" \"npm run dev:repo\"",
    "stop": "powershell -Command \"Get-Process -Name node -ErrorAction SilentlyContinue | Where-Object { $_.MainWindowTitle -eq '' } | Stop-Process -Force\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.91.0",
    "cors": "^2.8.5",
    "nodemailer": "^6.9.16",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "imagekit": "^6.0.0",
    "multer": "^1.4.5-lts.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "http-proxy-middleware": "^3.0.5",
    "nodemon": "^3.1.11"
  }
}
