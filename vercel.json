{
  "buildCommand": "npm install && cd Repository && npm install && npm run build && cd ../spotify-visualiser && npm install && npm run build",
  "installCommand": "npm install && cd Repository && npm install && cd ../spotify-visualiser && npm install",
  "outputDirectory": ".",
  "redirects": [
    {
      "source": "/spotify-visualiser/index.html",
      "destination": "/spotify-visualiser/dist/index.html",
      "permanent": false
    }
  ],
  "cleanUrls": false,
  "rewrites": [
    {
      "source": "/spotify-visualiser",
      "destination": "/spotify-visualiser/dist/index.html"
    },
    {
      "source": "/spotify-visualiser/",
      "destination": "/spotify-visualiser/dist/index.html"
    },
    {
      "source": "/spotify-visualiser/:path*",
      "destination": "/spotify-visualiser/dist/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/_astro/(.*)",
      "destination": "/Repository/dist/_astro/$1"
    },
    {
      "source": "/repo/images/artists/(.*)",
      "destination": "/Repository/dist/images/artists/$1"
    },
    {
      "source": "/repo/images/albums/(.*)",
      "destination": "/Repository/dist/images/albums/$1"
    },
    {
      "source": "/repo/images/icons/(.*)",
      "destination": "/Repository/dist/images/icons/$1"
    },
    {
      "source": "/repo/images/(.*)",
      "destination": "/Repository/dist/images/$1"
    },
    {
      "source": "/repo/_astro/(.*)",
      "destination": "/Repository/dist/_astro/$1"
    },
    {
      "source": "/repo/favicon/(.*)",
      "destination": "/Repository/dist/favicon/$1"
    },
    {
      "source": "/repo",
      "destination": "/Repository/dist/index.html"
    },
    {
      "source": "/repo/",
      "destination": "/Repository/dist/index.html"
    },
    {
      "source": "/repo/:path*",
      "destination": "/Repository/dist/:path*"
    }
  ],
  "headers": [
    {
      "source": "/spotify-visualiser/src/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    },
    {
      "source": "/repo/images/(.*\\.webp)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/webp"
        }
      ]
    },
    {
      "source": "/repo/images/(.*\\.png)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/png"
        }
      ]
    },
    {
      "source": "/repo/images/(.*\\.jpg)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/jpeg"
        }
      ]
    },
    {
      "source": "/repo/images/(.*\\.jpeg)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/jpeg"
        }
      ]
    }
  ]
}
