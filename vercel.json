{
  "buildCommand": "npm install && cd Repository && npm install && npm run build",
  "installCommand": "npm install && cd Repository && npm install",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/repo",
      "destination": "/Repository/dist/index.html"
    },
    {
      "source": "/repo/",
      "destination": "/Repository/dist/index.html"
    },
    {
      "source": "/repo/:path*",
      "destination": "/Repository/dist/:path*"
    }
  ]
}
